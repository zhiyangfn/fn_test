# Tool invocations
ble_mesh_light.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: TC32 C Linker'
	tc32-elf-ld --gc-sections -L"F:\MX-Project\MX_Software_V1.D\MeshLight8267-demo\ble_mesh_release_20171110_V1.D\sdk\proj_lib" -T ../boot.link -o"ble_mesh_light.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

boot-.lst: ble_mesh_light.elf
	@echo 'Invoking: TC32 Create Extended Listing'
	tc32-elf-objdump -x -D -l -S ble_mesh_light.elf  >"boot-.lst"
	@echo 'Finished building: $@'
	@echo ' '

light_8266.bin: ble_mesh_light.elf
	@echo 'Create Flash image (binary format)'
	tc32-elf-objcopy -O binary ble_mesh_light.elf  "light_8266.bin"
	@echo 'Finished building: $@'
	@echo ' '

sizedummy: ble_mesh_light.elf
	@echo 'Invoking: Print Size'
	tc32-elf-size -t ble_mesh_light.elf
	@echo 'Finished building: $@'
	@echo ' '

